<?xml version="1.0" encoding="utf-8"?>
<Page
    x:Class="RPISVR_Managements.Student_Informations.Check_Student_Informations.Check_Student_Info"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:RPISVR_Managements.Student_Informations.Check_Student_Informations"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    DataContext="{Binding StudentViewModel,Mode=TwoWay}"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
    xmlns:converters="using:RPISVR_Managements.Loading_View">

    <Page.Resources>
        <converters:BooleanToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    </Page.Resources>

    <Grid>
        
        <Grid.RowDefinitions>
            <RowDefinition x:Name="Text_Header" Height="Auto"/>
            <RowDefinition x:Name="Student_Count_Information" Height="220"/>
            <RowDefinition x:Name="Information_Show" Height="*"/>
        </Grid.RowDefinitions>
        <!--<Grid.ColumnDefinitions>
            <ColumnDefinition x:Name="Count_Data_Student_In_Skill" Width="Auto"/>
            <ColumnDefinition x:Name="Card_Student_info" Width="350"/>
            <ColumnDefinition x:Name="Table_Show_Information" Width="5*"/>
        </Grid.ColumnDefinitions>-->

        <StackPanel>
            <StackPanel x:Name="T_Header" Grid.Row="0">
                <TextBlock Text="ពិនិត្យទិន្នន័យសិស្សនិស្សិត"
                           Style="{StaticResource Font_KH_Body}"
                           Margin="10"
                           FontFamily="Khmer OS Muol Light"/>
            </StackPanel>

            <ScrollViewer Grid.Row="1" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Disabled" 
                  HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                <StackPanel Orientation="Horizontal">

                    <!-- Card 1 -->
                    <Border Background="Red" Padding="5" BorderBrush="Aqua" BorderThickness="3" CornerRadius="15" Margin="10">
                        <Border Background="Aqua" Padding="10" BorderBrush="Blue" BorderThickness="3" CornerRadius="10">
                            <!-- Inner Border: Aqua with your content inside -->
                            <StackPanel Height="150" >
                                <Image Source="/Assets/Check_Student_Info_Icon/icons8-student-100.png" Width="50" Height="50" />
                                <TextBlock x:Name="Total_Student_Count" 
                                               Text="" 
                                               Style="{StaticResource Font_KH_Title}"
                                               FontFamily="Khmer OS Muol Light"
                                               Foreground="Blue" 
                                               FontSize="20" 
                                               FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Student_Female_Count" 
                                           Text=""
                                           Style="{StaticResource Font_KH_Title}"
                                            Foreground="Green" 
                                            FontSize="16" 
                                            FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Student_Male_Count" 
                                           Text=""
                                           Style="{StaticResource Font_KH_Title}"
                                            Foreground="Green" 
                                            FontSize="16" 
                                            FontWeight="Bold"/>
                            </StackPanel>
                        </Border>
                    </Border>

                    <!-- Card 2 -->
                    <Border Background="Red" Padding="5" BorderBrush="Aqua" BorderThickness="3" CornerRadius="15" Margin="10">
                        <Border Background="Aqua" Padding="10" BorderBrush="Blue" BorderThickness="3" CornerRadius="10">
                            <!-- Inner Border: Aqua with your content inside -->
                            <StackPanel Height="150" >
                                <Image Source="/Assets/Check_Student_Info_Icon/icons8-flying-mortarboard-100.png" Width="50" Height="50" />
                                <TextBlock x:Name="Total_Stu_Count_Bachelor"
                                           Text="" 
                                           Style="{StaticResource Font_KH_Title}"
                                           FontFamily="Khmer OS Muol Light"
                                           Foreground="Blue" 
                                           FontSize="20" 
                                           FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Stu_Female_Count_Bachelor" 
                                           Text=""
                                            Style="{StaticResource Font_KH_Title}"
                                            Foreground="Green" 
                                            FontSize="16" 
                                            FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Student_Male_Count_Bachelor" 
                                            Text=""
                                            Style="{StaticResource Font_KH_Title}"
                                            Foreground="Green" 
                                            FontSize="16" 
                                            FontWeight="Bold"/>
                            </StackPanel>
                        </Border>
                    </Border>

                    <!-- Card 3 -->
                    <Border Background="Red" Padding="5" BorderBrush="Aqua" BorderThickness="3" CornerRadius="15" Margin="10">
                        <Border Background="Aqua" Padding="10" BorderBrush="Blue" BorderThickness="3" CornerRadius="10">
                            <!-- Inner Border: Aqua with your content inside -->
                            <StackPanel Height="150" >
                                <Image Source="/Assets/Check_Student_Info_Icon/icons8-diploma-100.png" Width="50" Height="50" />
                                <TextBlock x:Name="Total_Stu_Count_Associate"
                                           Text="" 
                                           Style="{StaticResource Font_KH_Title}"
                                           FontFamily="Khmer OS Muol Light"
                                           Foreground="Blue" 
                                           FontSize="20" 
                                           FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Stu_Female_Count_Associate" 
                                           Text=""
                                    Style="{StaticResource Font_KH_Title}"
                                    Foreground="Green" 
                                    FontSize="16" 
                                    FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Student_Male_Count_Associate" 
                                           Text=""
                                        Style="{StaticResource Font_KH_Title}"
                                        Foreground="Green" 
                                        FontSize="16" 
                                        FontWeight="Bold"/>
                            </StackPanel>
                        </Border>
                    </Border>

                    <!-- Card 4 -->
                    <Border Background="Green" Padding="5" BorderBrush="Aqua" BorderThickness="3" CornerRadius="15" Margin="10">
                        <Border Background="Aqua" Padding="10" BorderBrush="Blue" BorderThickness="3" CornerRadius="10">
                            <!-- Inner Border: Aqua with your content inside -->
                            <StackPanel Height="150" >
                                <StackPanel Orientation="Horizontal">
                                    <Image Source="/Assets/Check_Student_Info_Icon/icons8-degree-103.png" Width="50" Height="50" />
                                    <TextBlock Text="C3"
                                    Foreground="OrangeRed" 
                                    FontSize="30" 
                                    FontWeight="Bold"/>
                                </StackPanel>

                                <TextBlock x:Name="Total_Stu_Count_C3"
                                    Text="" 
                                   Style="{StaticResource Font_KH_Title}"
                                   FontFamily="Khmer OS Muol Light"
                                   Foreground="Blue" 
                                   FontSize="18" 
                                   FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Stu_Female_Count_C3" 
                                           Text=""
                                    Style="{StaticResource Font_KH_Title}"
                                    Foreground="Green" 
                                    FontSize="16" 
                                    FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Student_Male_Count_C3"
                                           Text=""
                                    Style="{StaticResource Font_KH_Title}"
                                    Foreground="Green" 
                                    FontSize="16" 
                                    FontWeight="Bold"/>
                            </StackPanel>
                        </Border>
                    </Border>

                    <!-- Card 5 -->
                    <Border Background="Green" Padding="5" BorderBrush="Aqua" BorderThickness="3" CornerRadius="15" Margin="10">
                        <Border Background="Aqua" Padding="10" BorderBrush="Blue" BorderThickness="3" CornerRadius="10">
                            <!-- Inner Border: Aqua with your content inside -->
                            <StackPanel Height="150" >
                                <StackPanel Orientation="Horizontal">
                                    <Image Source="/Assets/Check_Student_Info_Icon/icons8-degree-103.png" Width="50" Height="50" />
                                    <TextBlock Text="C2"
                                            Foreground="OrangeRed" 
                                            FontSize="30" 
                                            FontWeight="Bold"/>
                                </StackPanel>

                                <TextBlock x:Name="Total_Stu_Count_C2" 
                                           Text="" 
                                   Style="{StaticResource Font_KH_Title}"
                                   FontFamily="Khmer OS Muol Light"
                                   Foreground="Blue" 
                                   FontSize="18" 
                                   FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Stu_Female_Count_C2" 
                                           Text=""
                                   Style="{StaticResource Font_KH_Title}"
                                    Foreground="Green" 
                                    FontSize="16" 
                                    FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Student_Male_Count_C2" 
                                           Text=""
                                   Style="{StaticResource Font_KH_Title}"
                                    Foreground="Green" 
                                    FontSize="16" 
                                    FontWeight="Bold"/>
                            </StackPanel>
                        </Border>
                    </Border>
                    <!-- Card 6 -->
                    <Border Background="Green" Padding="5" BorderBrush="Aqua" BorderThickness="3" CornerRadius="15" Margin="10">
                        <Border Background="Aqua" Padding="10" BorderBrush="Blue" BorderThickness="3" CornerRadius="10">
                            <!-- Inner Border: Aqua with your content inside -->
                            <StackPanel Height="150" >
                                <StackPanel Orientation="Horizontal">
                                    <Image Source="/Assets/Check_Student_Info_Icon/icons8-degree-103.png" Width="50" Height="50" />
                                    <TextBlock Text="C1"
                                    Foreground="OrangeRed" 
                                    FontSize="30" 
                                    FontWeight="Bold"/>
                                </StackPanel>

                                <TextBlock x:Name="Total_Stu_Count_C1" 
                                           Text="" 
                                       Style="{StaticResource Font_KH_Title}"
                                       FontFamily="Khmer OS Muol Light"
                                       Foreground="Blue" 
                                       FontSize="18" 
                                       FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Stu_Female_Count_C1" 
                                           Text=""
                                        Style="{StaticResource Font_KH_Title}"
                                        Foreground="Green" 
                                        FontSize="16" 
                                        FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Student_Male_Count_C1" 
                                           Text=""
                                        Style="{StaticResource Font_KH_Title}"
                                        Foreground="Green" 
                                        FontSize="16" 
                                        FontWeight="Bold"/>
                            </StackPanel>
                        </Border>
                    </Border>
                    <!-- Card 7 -->
                    <Border Background="Blue" Padding="5" BorderBrush="Aqua" BorderThickness="3" CornerRadius="15" Margin="10">
                        <Border Background="Aqua" Padding="10" BorderBrush="Red" BorderThickness="3" CornerRadius="10">
                            <!-- Inner Border: Aqua with your content inside -->
                            <StackPanel Height="150" >
                                <StackPanel Orientation="Horizontal">
                                    <Image Source="/Assets/Check_Student_Info_Icon/icons8-degree-103.png" Width="50" Height="50"/>
                                    <TextBlock Text="1.5M"
                                           Foreground="Purple" 
                                            FontSize="30" 
                                            FontWeight="Bold"/>
                                </StackPanel>

                                <TextBlock x:Name="Total_Stu_Count_1_5M" 
                                           Text="" 
                                   Style="{StaticResource Font_KH_Title}"
                                   FontFamily="Khmer OS Muol Light"
                                   Foreground="Blue" 
                                   FontSize="18" 
                                   FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Stu_Female_Count_1_5M" 
                                           Text=""
                                    Style="{StaticResource Font_KH_Title}"
                                    Foreground="Green" 
                                    FontSize="16" 
                                    FontWeight="Bold"/>
                                <TextBlock x:Name="Total_Student_Male_Count_1_5M" 
                                           Text=""
                                    Style="{StaticResource Font_KH_Title}"
                                    Foreground="Green" 
                                    FontSize="16" 
                                    FontWeight="Bold"/>
                            </StackPanel>
                        </Border>
                    </Border>
                </StackPanel>
            </ScrollViewer>


            <ScrollViewer Grid.Row="2" VerticalScrollMode="Enabled" HorizontalScrollMode="Disabled" VerticalScrollBarVisibility="Auto" VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Height="700" BorderBrush="Aqua" BorderThickness="3" Margin="10" CornerRadius="10">
                <StackPanel Orientation="Vertical" >
                    
                    <RelativePanel>

                        <TextBlock x:Name="Text_Total_Stu" Text="ទិន្នន័យសិស្សនិស្សិតសរុប តាមជំនាញនីមួយៗ (នាក់)"
                                   Style="{StaticResource Font_KH_Title}"
                                   Margin="10,10,0,0"
                                   FontFamily="Khmer OS Muol Light"/>
                        <ScrollViewer x:Name="Student_BarGraph" HorizontalScrollMode="Enabled" HorizontalScrollBarVisibility="Visible"
                                        HorizontalAlignment="Stretch" VerticalAlignment="Stretch" RelativePanel.Below="Text_Total_Stu" >

                            <StackPanel Width="2000" Height="400">
                                
                                <Canvas Name="ChartCanvas" Background="Gray"/>
                            </StackPanel>

                        </ScrollViewer>
                        
                        

                        <StackPanel x:Name="Student_Card" Width="350" Height="446" Background="#5556de" HorizontalAlignment="Left" Margin="10" CornerRadius="5" RelativePanel.Below="Student_BarGraph">
                            <RelativePanel>
                                <Image x:Name="Logo_RPI" Source="/Assets/RPISSVR_logo.png" Width="40" Height="40"/>
                                <TextBlock x:Name="Logo_Text_RIP" Text="វិទ្យាស្ថានពហុបច្ចេកទេសភូមិភាគតេជោសែនស្វាយរៀង"
                                           Style="{StaticResource Font_KH_Body}"
                                           Margin="5,10,0,0"
                                           FontFamily="Khmer OS Muol Light"
                                           FontSize="11"
                                           RelativePanel.RightOf="Logo_RPI"
                                           />
                                <Image x:Name="Stu_ShowImage"
                                         Width="120"
                                         Height="120"
                                         Source="{Binding Stu_Image_Source,Mode=TwoWay}"
                                         RelativePanel.Below="Logo_Text_RIP" 
                                         Margin="80,10,0,0"/>
                                <TextBlock x:Name="Stu_ID"                                              
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.Below="Logo_Text_RIP" 
                                           RelativePanel.RightOf="Stu_ShowImage"
                                           Margin="0,20,0,0"
                                           IsTextSelectionEnabled="True">
                                    <Run Text="{Binding Stu_IDShow,Mode=TwoWay}" Foreground="Aqua"/>
                                </TextBlock>
                                <TextBlock x:Name="Stu_Name"                                              
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.Below="Stu_ID" 
                                           RelativePanel.RightOf="Stu_ShowImage">
                                    <Run Text="ឈ្មោះ "/>
                                    <Run Text="{Binding Stu_FirstName_KH}" Foreground="Aqua"/>
                                </TextBlock>
                                <TextBlock Text="{Binding Stu_LastName_KH}"
                                           Style="{StaticResource Font_KH_Body}"
                                           Foreground="Aqua"
                                           RelativePanel.RightOf="Stu_Name"
                                           RelativePanel.Below="Stu_ID"
                                           Margin="5,0,0,0"/>
                                <TextBlock x:Name="Stu_Gender"
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.RightOf="Stu_ShowImage"
                                           RelativePanel.Below="Stu_Name">
                                    <Run Text="ភេទ"/>
                                    <Run Text="{Binding Stu_GenderShow}" Foreground="Aqua"/>
                                </TextBlock>
                                <TextBlock x:Name="Stu_Phone_Number"
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.RightOf="Stu_ShowImage"
                                           RelativePanel.Below="Stu_Gender"
                                           IsTextSelectionEnabled="True">
                                    <Run Text="លេខទូរស័ព្ទ (0)"/>
                                    <Run Text="{Binding Stu_PhoneNumber}" Foreground="Aqua"/>
                                </TextBlock>
                                <Line X1="10" Y1="10" X2="350" Y2="10" 
                                            x:Name="Line_1"
                                            Stroke="Aqua" 
                                            StrokeThickness="2"
                                            RelativePanel.Below="Stu_ShowImage"
                                            Margin="0,0,10,0"/>
                                <TextBlock x:Name="Stu_SubjectSkill"
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.Below="Line_1"
                                           Margin="10,5,0,0">
                                    <Run Text="ជំនាញ          :"/>
                                    <Run Text="{Binding Stu_EducationSubjects}" Foreground="Aqua"/>
                                </TextBlock>
                                <TextBlock x:Name="Stu_Edu_Level"
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.Below="Stu_SubjectSkill"
                                           Margin="10,0,0,0" >
                                    <Run Text="កម្រិតសិក្សា   :"/>
                                    <Run Text="{Binding Stu_EducationLevels}" Foreground="Aqua"/>
                                </TextBlock>
                                <TextBlock x:Name="Stu_Edu_Year"
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.Below="Stu_Edu_Level"
                                           Margin="10,0,0,0">
                                    <Run Text="ឆ្នាំទី              :"/>
                                    <Run Text="{Binding Stu_StudyingTime}" Foreground="Aqua"/>
                                </TextBlock>
                                <TextBlock x:Name="Stu_Edu_Semester"
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.Below="Stu_Edu_Year"
                                           Margin="10,0,0,0">
                                    <Run Text="ឆមាស          :"/>
                                    <Run Text="{Binding Stu_Semester}" Foreground="Aqua"/>
                                </TextBlock>
                                <TextBlock x:Name="Stu_Study_Time"
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.Below="Stu_Edu_Semester"
                                           Margin="10,0,0,0">
                                    <Run Text="វេនសិក្សា       :"/>
                                    <Run Text="{Binding Stu_StudyTimeShift}" Foreground="Aqua"/>

                                </TextBlock>
                                <TextBlock x:Name="Stu_StudyType"
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.Below="Stu_Study_Time"
                                           Margin="10,0,0,0">
                                    <Run Text="ប្រភេទសិក្សា  :"/>
                                    <Run Text="{Binding Stu_EducationType}" Foreground="Aqua"/>
                                </TextBlock>
                                <TextBlock x:Name="Stu_StudyYear"
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.Below="Stu_StudyType"
                                           Margin="10,0,0,0">
                                    <Run Text="ឆ្នាំសិក្សា        :"/>
                                    <Run Text="{Binding Stu_StudyYear}" Foreground="Aqua"/>

                                </TextBlock>
                                <Line X1="10" Y1="10" X2="350" Y2="10" 
                                        x:Name="Line_2"
                                        Stroke="Aqua" 
                                        StrokeThickness="2"
                                        RelativePanel.Below="Stu_StudyYear"
                                        Margin="0,0,10,0"/>
                                <TextBlock x:Name="Stu_Birth_Date"
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.Below="Line_2"
                                           Margin="10,0,0,0">
                                    <Run Text="ថ្ងៃខែឆ្នាំកំណើត          :"/>
                                    <Run Text="{Binding Stu_BirthdayDateShow, Mode=TwoWay}" Foreground="Aqua"/>
                                </TextBlock>
                                <TextBlock x:Name="Stu_Live_Place"
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.Below="Stu_Birth_Date"
                                           Margin="10,0,0,0">
                                    <Run Text="អាស័យដ្ឋានបច្ចុប្បន្ន    :"/>
                                    <Run Text="{Binding Stu_Live_Vill}" Foreground="Aqua"/>
                                    <Run Text="{Binding Stu_Live_Comm}" Foreground="Aqua"/>
                                </TextBlock>
                                <TextBlock x:Name="Stu_Live_Place2"
                                           Style="{StaticResource Font_KH_Body}"
                                           RelativePanel.Below="Stu_Live_Place"
                                           Margin="130,0,0,0">
                                    <Run Text="{Binding Stu_Live_Dis}" Foreground="Aqua"/>
                                    <Run Text="{Binding Stu_Live_Pro}" Foreground="Aqua"/>
                                </TextBlock>
                                           

                            </RelativePanel>
                        </StackPanel>

                        <StackPanel x:Name="Student_Button_Search" RelativePanel.RightOf="Student_Card" RelativePanel.Below="Student_BarGraph">
                            <RelativePanel>
                                <ScrollView x:Name="Search_Information" HorizontalScrollBarVisibility="Auto" 
                                                HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                    <StackPanel Orientation="Horizontal">
                                        <TextBox x:Name="Search_ID_Name"
                                                  Header="ស្វែងរកលេខសម្គាល់,ឈ្មោះ"
                                                  Text="{Binding SearchText_ID_Name,Mode =TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                  Style="{StaticResource Style_KH_TextBox}"
                                                  Margin="5,0,0,10">
                                        </TextBox>
                                        <ComboBox x:Name="Stu_EducationLevels_Search"
                                                    Header="កម្រិតសិក្សា"
                                                    ItemsSource="{Binding EducationsLevel_Combobox}"
                                                    DisplayMemberPath="Stu_EducationLevels"
                                                    SelectedValuePath="Stu_EducationLevels_ID"     
                                                    RelativePanel.RightOf="Search_ID_Name"
                                                    Style="{StaticResource Stye_KH_Combobox}"
                                                    Margin="5,0,0,10"
                                                    SelectedItem="{Binding SelectedEducationLevel_Stu_Info,Mode=TwoWay}"
                                                    >
                                        </ComboBox>
                                        <Button x:Name="btn_clear_edu_level"
                                                Content="x"
                                                Margin="0,20,0,0"  
                                                Foreground="Red"
                                                Click="clear_edu_level"/>
                                        <ComboBox x:Name="Stu_Subject_Search"
                                                    Header="ជំនាញ"    
                                                    ItemsSource="{Binding EducationSubjectSkill_Combobox}"
                                                    DisplayMemberPath="Stu_EducationSubjects"
                                                    SelectedValuePath="Stu_EducationSubject_ID"  
                                                    RelativePanel.RightOf="Stu_EducationLevels_Search"
                                                    Style="{StaticResource Stye_KH_Combobox}"
                                                    Margin="5,0,0,10"
                                                    SelectedItem="{Binding SelectedEducationSubjects_Stu_Info,Mode=TwoWay}"
                                                    >
                                        </ComboBox>
                                        <Button x:Name="btn_clear_edu_subject"
                                                    Content="x"
                                                    Margin="0,20,0,0"  
                                                    Foreground="Red"
                                                    Click="clear_edu_subject"/>
                                        <ComboBox x:Name="Stu_StudyTime_Search"
                                                Header="វេនសិក្សា"
                                                ItemsSource="{Binding EducationStudyTimeShift_Combobox}"
                                                DisplayMemberPath="Stu_StudyTimeShift"
                                                SelectedValuePath="Stu_StudyTimeShift_ID"   
                                                RelativePanel.RightOf="Stu_Subject_Search"
                                                Style="{StaticResource Stye_KH_Combobox}"
                                                Margin="5,0,0,5"
                                                SelectedItem="{Binding SelectedStu_StudyTimeShift_Stu_Info,Mode=TwoWay}">
                                        </ComboBox>
                                        <Button x:Name="btn_clear_edu_studytime"
                                                        Content="x"
                                                        Margin="0,20,0,0"  
                                                        Foreground="Red"
                                                        Click="clear_edu_studytime"/>

                                        <ComboBox x:Name="Stu_TypeStudy_Search"
                                                    Header="ប្រភេទសិក្សា"
                                                    ItemsSource="{Binding EducationStudyType_Combobox}"
                                                    DisplayMemberPath="Stu_EducationType"
                                                    SelectedValuePath="Stu_EducationType_ID"
                                                    RelativePanel.RightOf="Stu_StudyTime_Search"
                                                    Style="{StaticResource Stye_KH_Combobox}"
                                                    Margin="5,0,0,5"
                                                    SelectedItem="{Binding SelectedStu_EducationType_Stu_Info,Mode=TwoWay}">
                                        </ComboBox>
                                        <Button x:Name="btn_clear_edu_typestudy"
                                                    Content="x"
                                                    Margin="0,20,0,0"  
                                                    Foreground="Red"
                                                    Click="clear_edu_typestudy"/>
                                        <ComboBox x:Name="Stu_StudyYear_Search"
                                                    Header="ឆ្នាំសិក្សា"
                                                    ItemsSource="{Binding EducationStudyYear_Combobox}"
                                                    DisplayMemberPath="Stu_StudyYear" 
                                                    SelectedValuePath="Stu_StudyYear_ID"
                                                    RelativePanel.RightOf="Stu_TypeStudy_Search"
                                                    Style="{StaticResource Stye_KH_Combobox}"
                                                    Margin="5,0,0,5"
                                                    SelectedItem="{Binding SelectedStu_StudyYear_Stu_Info,Mode=TwoWay}">
                                        </ComboBox>
                                        <Button x:Name="btn_clear_edu_studyyear"
                                                    Content="x"
                                                    Margin="0,20,0,0"  
                                                    Foreground="Red"
                                                    Click="clear_edu_studyyear"/>

                                        <Button Command="{Binding Search_Stu_Info}" Margin="5,20,0,0" Background="Green">
                                            <Button.Content>
                                                <Image Source="/Assets/Check_Student_Info_Icon/icons8-search-96.png" Width="23" Height="23"/>
                                            </Button.Content>
                                        </Button>
                                        <Button Click="Clear" Margin="5,20,5,0" Background="Red">
                                            <Button.Content>
                                                <Image Source="/Assets/Check_Student_Info_Icon/icons8-clear-search-96.png" Width="23" Height="23" />
                                            </Button.Content>
                                        </Button>
                                    </StackPanel>
                                </ScrollView>



                                <StackPanel x:Name="Table_Stu_Info_Header" RelativePanel.Below="Search_Information" Orientation="Horizontal" >
                                    <TextBlock HorizontalAlignment="Left" Text="តារាងទិន្នន័យសិស្សនិស្សិត" Style="{StaticResource Font_KH_Title}" FontSize="16" FontWeight="Bold" Margin="0,10,0,10"/>
                                    <StackPanel HorizontalAlignment="Right" VerticalAlignment="Center" Orientation="Horizontal" Margin="40,0,0,0">
